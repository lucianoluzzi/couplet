<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/onboardingFragment"
        android:name="com.couplesdating.couplet.ui.onboarding.letsStart.OnboardingFragment"
        android:label="OnboardingFirstFragment">
        <action
            android:id="@+id/action_onboardingFragment_to_learnFromProfessionalsFragment"
            app:destination="@id/learnFromProfessionalsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/learnFromProfessionalsFragment"
        android:name="com.couplesdating.couplet.ui.onboarding.learnFromProfessionals.LearnFromProfessionalsOnboardingFragment"
        android:label="LearnFromProfessionalsOnboardingFragment">
        <action
            android:id="@+id/action_learnFromProfessionalsFragment_to_mildToWildOnboardingFragment"
            app:destination="@id/mildToWildOnboardingFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/mildToWildOnboardingFragment"
        android:name="com.couplesdating.couplet.ui.onboarding.mildToWild.MildToWildOnboardingFragment"
        android:label="MildToWildOnboardingFragment">
        <action
            android:id="@+id/action_mildToWildOnboardingFragment_to_privacyOnboardingFragment"
            app:destination="@id/privacyOnboardingFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/privacyOnboardingFragment"
        android:name="com.couplesdating.couplet.ui.onboarding.privacy.PrivacyOnboardingFragment"
        android:label="PrivacyOnboardingFragment">
        <action
            android:id="@+id/action_privacyOnboardingFragment_to_intimateOnboardingFragment"
            app:destination="@id/intimateOnboardingFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/intimateOnboardingFragment"
        android:name="com.couplesdating.couplet.ui.onboarding.intimate.IntimateOnboardingFragment"
        android:label="IntimateOnboardingFragment">
        <action
            android:id="@+id/action_intimateOnboardingFragment_to_socialLoginFragment"
            app:destination="@id/socialLoginFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/socialLoginFragment"
        android:name="com.couplesdating.couplet.ui.login.socialLogin.SocialLoginFragment"
        android:label="SocialLoginFragment">
        <action
            android:id="@+id/action_socialLoginFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_socialLoginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.couplesdating.couplet.ui.login.emailLogin.LoginFragment"
        android:label="LoginFragment">
        <action
            android:id="@+id/action_loginFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment" />
        <action
            android:id="@+id/action_loginFragment_to_emailAndPasswordFragment"
            app:destination="@id/emailAndPasswordFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/emailAndPasswordFragment"
        android:name="com.couplesdating.couplet.ui.register.emailAndPassword.RegisterFragment"
        android:label="EmailAndPasswordFragment">
        <action
            android:id="@+id/action_emailAndPasswordFragment_to_nameAndGenderFragment"
            app:destination="@id/nameAndGenderFragment" />
    </fragment>
    <fragment
        android:id="@+id/nameAndGenderFragment"
        android:name="com.couplesdating.couplet.ui.register.nameAndGender.NameAndGenderFragment"
        android:label="NameAndGenderFragment">
        <action
            android:id="@+id/action_nameAndGenderFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgotPasswordFragment"
        android:name="com.couplesdating.couplet.ui.login.forgotPassword.ForgotPasswordFragment"
        android:label="ForgotPasswordFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.couplesdating.couplet.ui.home.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_socialLoginFragment"
            app:destination="@id/socialLoginFragment" />
        <action
            android:id="@+id/action_homeFragment_to_onboardingFragment"
            app:destination="@id/onboardingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_dashboardFragment"
            app:destination="@id/dashboardFragment" />
        <argument
            android:name="user"
            app:argType="com.couplesdating.couplet.domain.model.User"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <fragment
        android:id="@+id/errorFragment"
        android:name="com.couplesdating.couplet.ui.error.ErrorFragment"
        android:label="ErrorFragment" />
    <fragment
        android:id="@+id/invitePartnerFragment"
        android:name="com.couplesdating.couplet.ui.invite.InvitePartnerFragment"
        android:label="InvitePartnerFragment" />
    <fragment
        android:id="@+id/invitedFragment"
        android:name="com.couplesdating.couplet.ui.invited.InvitedFragment"
        android:label="InvitedFragment">
        <deepLink
            android:id="@+id/deepLink"
            android:autoVerify="true"
            app:uri="https://couplet.love/partner?id={id}&amp;inviteId={inviteId}&amp;displayName={displayName}&amp;note={note}" />
        <argument
            android:name="id"
            app:argType="string" />
        <argument
            android:name="inviteId"
            app:argType="string" />
        <argument
            android:name="displayName"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="note"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_invitedFragment_to_socialLoginFragment"
            app:destination="@id/socialLoginFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_right"
            app:popExitAnim="@anim/slide_out_left"
            app:popUpTo="@id/invitedFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.couplesdating.couplet.ui.dashboard.DashboardFragment"
        android:label="DashboardFragment">
        <action
            android:id="@+id/action_dashboardFragment_to_invitePartnerFragment"
            app:destination="@id/invitePartnerFragment" />
        <argument
            android:name="user"
            app:argType="com.couplesdating.couplet.domain.model.User"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>

</navigation>