<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/socialLoginFragment">

    <fragment
        android:id="@+id/socialLoginFragment"
        android:name="com.couplesdating.couplet.ui.SocialLoginFragment"
        android:label="SocialLoginFragment" >
        <action
            android:id="@+id/action_socialLoginFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_socialLoginFragment_to_emailAndPasswordFragment"
            app:destination="@id/emailAndPasswordFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.couplesdating.couplet.ui.login.emailLogin.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/emailAndPasswordFragment"
        android:name="com.couplesdating.couplet.ui.register.emailAndPassword.EmailAndPasswordFragment"
        android:label="EmailAndPasswordFragment" >
        <action
            android:id="@+id/action_emailAndPasswordFragment_to_nameAndGenderFragment"
            app:destination="@id/nameAndGenderFragment" />
    </fragment>
    <fragment
        android:id="@+id/nameAndGenderFragment"
        android:name="com.couplesdating.couplet.ui.register.nameAndGender.NameAndGenderFragment"
        android:label="NameAndGenderFragment" >
        <action
            android:id="@+id/action_nameAndGenderFragment_to_termsOfUsage"
            app:destination="@id/termsOfUsage" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.couplesdating.couplet.ui.home.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/termsOfUsage"
        android:name="com.couplesdating.couplet.ui.register.termsOfUsage.TermsOfUsageFragment"
        android:label="TermsOfUsage" >
        <action
            android:id="@+id/action_termsOfUsage_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>